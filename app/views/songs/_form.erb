
<%= form_for @song do |f| %>

<p> <%= f.label :song_title %><br>
<%= f.text_field :title %><br>
</p>
<p> <%= f.label :released %><br>
<%= f.check_box :released %><br>
</p>
<p> <%= f.label :release_year %><br>
<%= f.number_field :release_year %><br>
</p>
<p> <%= f.label :artist_name %><br>
<%= f.text_field :artist_name %><br>
</p>
<p> <%= f.label :genre %><br>
<%= f.text_field :genre %><br>
</p>

<%= f.submit%>

<%end%>
